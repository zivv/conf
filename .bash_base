# To add this to the config, run:
# echo "[[ -f ~/.bash_base ]] && . ~/.bash_base" >> ~/.bashrc

# Use 'unalias' to cancle
alias v='vim'
alias vd='vimdiff'
alias s='source ~/.bashrc'
alias g='git'
alias tma='tmux attach'

function fig() {
  find $1 | grep $2
}

function fxgr() {
  find $1 | xargs grep $2
}

[[ -f ~/.bash_local ]] && . ~/.bash_local
