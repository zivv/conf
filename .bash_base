# To add this to the config,
# if in Linux:
#     echo "[[ -f ~/.bash_base ]] && . ~/.bash_base" >> ~/.bashrc
# if in OSX:
#     echo "[[ -f ~/.bash_base ]] && . ~/.bash_base" >> ~/.bash_profile

# Use 'unalias' to cancle
alias v='vim'
alias vd='vimdiff'
alias g='git'
alias tma='tmux attach'

function figr() {
  find $1 | grep $2
}

[[ -f ~/.bash_local ]] && . ~/.bash_local
