alias s='source $BASH_FILE'


# keep same keymapping with zsh
alias ~='cd ~'
alias ..='cd ..'
alias ...='.. && ..'
alias ....='... && ..'

alias su='sudo'

alias ga='g a'
alias ga.='g a .'
alias gcb='g co -b'
alias gco-='g co --'
alias gc-m='g ci -m'
alias gd='g d'
alias gdc='g dc'
alias gdh='g dh'
alias gdt='g dt'
alias gl='g pl'
alias gp='g ps'
alias gst='g st'

alias ta='tmux attach -t'
alias ts='tmux new-session -s'
alias tl='tmux list-sessions'


if pip show powerline-status > /dev/null ; then
  powerline-daemon -q
  POWERLINE_BASH_CONTINUATION=1
  POWERLINE_BASH_SELECT=1
  . $POWERLINE_ROOT/powerline/bindings/bash/powerline.sh
fi
